<div class="js-template-panel-view">
  <div class="js-SidebarPane_title SidebarPane_title <%= panel.collapsed ? '' : 'expanded' %>" data-id="<%= panel.tabName %>">
    Difference From Current DOM
  </div>
  <% if (!panel.collapsed && panel.view) { %>
    <div class="SidebarPane_data visible">
      <pre class="TemplateString"><code><%= panel.view.templateDiff %></code></pre>
    </div>
  <% } %>
</div>

<div class="template_info_panel">
  <div class="js-sidebar-pane-title sidebar-pane-title <%= panel.collapsed ? '' : 'expanded' %>" data-id="<%= panel.tabName %>">
    VDOM Template
  </div>
  <% if (!panel.collapsed && panel.view) { %>
    <div class="sidebar-pane visible">
      <pre class="template_string"><code class="monospace-lg"><%= panel.view.vdomTemplate %></code></pre>
    </div>
  <% } %>
</div>

<div class="model_info_panel">
  <div class="js-SidebarPane_title SidebarPane_title <%= panel.collapsed ? '' : 'expanded' %>" data-id="<%= panel.tabName %>">
    Model
  </div>
  <% if (!panel.collapsed && panel.model) { %>
    <div class="SidebarPane_data visible">
      <div>
        <span class="name">debugName</span>
        <span>:</span>
        <span class="value"> <%= panel.model.debugName %> </span>
      </div>
      <% if (panel.model.obj.parentProp) { %>
      <div>
        <span class="name">parentProp</span>
        <span>:</span>
        <span class="value"> <%= panel.model.obj.parentProp %> </span>
      </div>
      <% } %>
    </div>
  <% } %>
</div>

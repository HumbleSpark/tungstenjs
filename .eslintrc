{
  "rules": {
    "indent": [
      2,
      2,
      {"indentSwitchCase": true}
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "require": true,
    "module": true
  },
  "extends": "eslint:recommended"
}